<h1> New experiment! </h1>

<%= form_for :experiment, url: proposal_experiments_path do |f| %>
<ul>
  <ul><%= f.text_area :results, cols: "40", rows: '12', placeholder: 'Results' %></ul>
  <ul><%= f.text_area :conclusions, cols: "40", rows: '12', placeholder:'Conclusions- Not necessary'  %></ul>
  <ul><%= f.submit %></ul>
<% end %>

<% if @errors %>
  <% @errors.each do |error| %>
    <%= error %>
  <% end %>
<% end %>
